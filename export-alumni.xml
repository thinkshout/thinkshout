---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "https://thinkshout.com/feed.xml" }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% assign team = site.team | where: "current-employee", "false" %}
      {% for person in team %}
      <item>
        <title>ThinkShout Alumnus</title>
        <description>{{ person.description }}</description>
        <slug>{{ person.slug }}</slug>
        <first_name>{{ person.first_name }}</first_name>
        <last_name>{{ person.last_name }}</last_name>
        <leadin>{{ person.leadin | xml_escape }}</leadin>
        <bio>{{ person.bio | xml_escape }}</bio>
        <link>https://thinkshout.com{{ person.url }}</link>
        <image>https://thinkshout.com/assets/images/{{ person.team-image.large }}</image>
        <guid isPermaLink="true">https://thinkshout.com{{ person.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
